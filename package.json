{
  "name": "ionic-starter-kit",
  "version": "1.0.0",
  "description": "Ionic 1 Starter Kit",
  "license": "MIT",
  "main": "app/index.html",
  "scripts": {
    "start": "webpack-dev-server --colors --json --progress --config webpack/dev.config.js",
    "build:development": "rm -rf www/* && webpack --release --config webpack/dev.config.js",
    "build:production": "rm -rf www/* && webpack --release --config webpack/prod.config.js",
    "production": "rm -rf www/* && webpack --release --config webpack/prod.config.js && ionic serve",
    "android": "rm -rf www/* && webpack --release --config webpack/prod.config.js && ionic run android",
    "ios": "rm -rf www/* && webpack --release --config webpack/prod.config.js && ionic run ios",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hawatel/ionic1-starter.git"
  },
  "homepage": "https://github.com/Hawatel/ionic1-starter",
  "bugs": "https://github.com/Hawatel/ionic1-starter/issues",
  "keywords": [
    "ionic",
    "cordova",
    "angular",
    "angularjs",
    "babel6",
    "live",
    "webpack",
    "hawatel"
  ],
  "dependencies": {
    "ionic-sdk": "^1.2.4",
    "ng-cordova": "^0.1.26-alpha"
  },
  "devDependencies": {
    "angular": "^1.4.3",
    "angular-mocks": "^1.4.3",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "deep-extend": "^0.4.1",
    "file-loader": "^0.8.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^1.5.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.1",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.9.3",
    "mocha": "^2.4.5",
    "ng-annotate-webpack-plugin": "git://github.com/jeffling/ng-annotate-webpack-plugin.git",
    "node-sass": "^3.7.0",
    "path": "^0.12.7",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-config": "^4.0.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-console",
    "cordova-plugin-device",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "cordova-plugin-whitelist",
    "ionic-plugin-keyboard",
    "cordova-plugin-geolocation",
    "cordova-plugin-inappbrowser",
    "cordova-plugin-network-information",
    "cordova.plugins.diagnostic@2.3.17"
  ],
  "cordovaPlatforms": [
    "android",
    "ios"
  ]
}
